<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Propriétaire</p>
    <i class="fas fa-male"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside"><%= @house.owner.first_name %></div>
    <div class="wrap-inside"><%= @house.owner.last_name %></div>
    <div class="wrap-inside"><%= @house.owner.email %></div>
    <div class="wrap-inside"><%= @house.owner.phone_number%></div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Intérieur du bien</p>
    <i class="fas fa-home"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">Total: <%= @house.surface_total%> m2</div>
    <div class="wrap-inside">Habitable: <%= @house.surface_living%> m2</div>
    <div class="wrap-inside">Carrez: <%= @house.surface_Carrez%> m2</div>
  </div>
  <div class ="wrap">
    <div class="wrap-inside"><%= @house.nb_rooms %> pièces</div>
    <div class="wrap-inside"><%= @house.nb_bedrooms %> chambres</div>
    <div class="wrap-inside"><%= @house.nb_livingrooms%> séjours</div>
    <div class="wrap-inside"><%= @house.nb_bathrooms%> salles de bain</div>
    <div class="wrap-inside"><%= @house.equiped_kitchen ? "Cuisine équipée" : "Cuisine non équipée" %></div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Extérieur du bien</p>
    <i class="fas fa-leaf"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">Total: <%= @house.surface_total%> m2</div>
    <div class="wrap-inside">Habitable: <%= @house.surface_living%> m2</div>
    <div class="wrap-inside">Carrez: <%= @house.surface_Carrez%> m2</div>
  </div>
  <div class ="wrap">
    <div class="wrap-inside"><%= @house.nb_rooms %> pièces</div>
    <div class="wrap-inside"><%= @house.nb_bedrooms %> chambres</div>
    <div class="wrap-inside"><%= @house.nb_livingrooms%> séjours</div>
    <div class="wrap-inside"><%= @house.nb_bathrooms%> salles de bain</div>
    <div class="wrap-inside"><%= @house.equiped_kitchen ? "Cuisine équipée" : "Cuisine non équipée" %></div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Copopriété</p>
    <i class="fas fa-users"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">Bien en copropriété : Oui</div>
    <div class="wrap-inside">Nombre de lots : 30</div>
    <div class="wrap-inside">Charges annuelles : 300€</div>
  </div>
  <div class ="wrap">
    <div class="wrap-inside">Procédure en cours : Non</div>
    <div class="wrap-inside">Surface Carrez : 95 m2</div>
  </div>
  <div class ="wrap">
    <div class="wrap-inside">Informations particulières : Belle maison</div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Aspects financiers</p>
    <i class="fas fa-money-bill"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">Charges : éléctricité + eau</div>
    <div class="wrap-inside">Taxe fonicère : 480€</div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Diagnostique énergitique</p>
    <i class="fab fa-envira"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">DPE effectué : oui</div>
    <div class="wrap-inside">Consommation : 574 Kwh/m2/an</div>
    <div class="wrap-inside">Emission GES : 7kg co2/m2/an</div>
  </div>
</div>

<div class="white-back z-depth-3">
  <div class="title-icon">
    <p>Prix de vente et rémunération</p>
    <i class="fas fa-euro-sign"></i>
  </div>
  <div class="wrap">
    <div class="wrap-inside">Prix TTC : 120 000€</div>
    <div class="wrap-inside">Prix net vendeur : 110 000€</div>
  </div>
  <div class ="wrap">
    <div class="wrap-inside">Charges : acquereur</div>
    <div class="wrap-inside">Honnoraires TTC : 10 000€</div>
  </div>
</div>
<div id="mapid" class="z-depth-3" data-markers=" <%= @markers.to_json %> "></div>
